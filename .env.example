# OpenRouter API Configuration
OPENROUTER_API_KEY=your_api_key_here

# Comma-separated list of models to use
# First half will be used for scenario generation, second half for reasoning
MODELS=anthropic/claude-3.5-sonnet,openai/gpt-4-turbo

# Worker Configuration
MIN_WORKERS=4
MAX_WORKERS=32

# Batch Configuration
BATCH_SIZE=1000
VALIDATION_RETRIES=3
RATE_LIMIT_CHECK_INTERVAL=5

# Storage Configuration
DB_PATH=data/training_data.db
OUTPUT_DIR=output
